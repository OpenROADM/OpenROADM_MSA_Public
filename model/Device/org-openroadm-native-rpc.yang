module org-openroadm-native-rpc {
  namespace "http://org/openroadm/native/rpc";
  prefix org-openroadm-native-rpc;

  import org-openroadm-common-types {
    prefix org-openroadm-common-types;
    revision-date 2025-03-28;
  }

  organization
    "Open ROADM MSA";
  contact
